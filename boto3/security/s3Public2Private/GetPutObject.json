{
  "Version" : "2012-10-17",
  "Statements":
  [
    {
      "Resource": "arn:aws:s3::<account-number>-private-bucket",
      "Effect": "Allow",
      "Action":
      [
        "s3:GetObjectAcl",
        "s3:PutObjectAcl"
      ]
    },

    {
      "Resource": "arn:aws:logs:*:*:*",
      "Effect": "Allow",
      "Action":
      [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ]
    }
  ]
}